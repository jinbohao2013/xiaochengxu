  border-top: 1px solid #FFC64C;
  margin-bottom: 20rpx;
}
.loading{
    display: inline-block;
    margin-right: 12rpx;
    vertical-align: middle;
    width: 20rpx;
    height: 20rpx;
    background: transparent;
    border-radius: 50%;
    border: 2px solid #FDD000;
    border-color: #FDD000 #FDD000 #FDD000 transparent;
    animation: loading-spin 0.6s linear;
    animation-iteration-count: infinite;
}

@keyframes loading-spin {
    0% {
        transform: rotate(0);
    }
    100% {
        transform: rotate(360deg);
    }
}
.toTop{
  position: fixed;
  right: 20rpx;
  bottom: 120rpx;
  border: 1px solid #eee;
  background-color: #fff;
  border-radius: 50%;
  padding: 15rpx;
  box-shadow: 1px 1px 4px #eee;
}
.toTop .iconfont{
  font-size: 40rpx;
}
